create table historico_aluguel(
	id int auto_increment primary key,
    tipo_vaga varchar(20) not null,
    modelo varchar(20), 
	Placa varchar(7) not null,
	codigo_vaga varchar(20) not null,
	hora_entrada TIMESTAMP not null,
	hora_saida TIMESTAMP not null,
	valor_cobrado decimal not null	
); 
